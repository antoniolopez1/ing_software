<h1>EmployeePayment#index</h1>
<table class="table ">
	  <thead class="thead-dark">
	<tr><TH>ID</TH>
		<TH>Personal</TH>
		<TH>Salary_id</TH>
		<TH>importe</TH>
		<TH>saldo</TH>
		<TH>fecha</TH>
				
		<TH colspan="2"><%= link_to 'Nuevo', employee_payment_new_path %></TH></tr>
</thead>
	<tbody>
	<% @employee_payments.each do |f| %>
	
	<tr><TD><%= f.id %></TD>
		<TD><%= f.salary.employee.lastname %></TD>
		<TD><%= f.salary_id %></TD>
		<TD><%= f.amount %></TD>
		<TD><%= f.balance %></TD>
		<TD><%= f.created_at.strftime("%Y/%m/%d") %></TD>
		
		
		<TD><%= link_to 'Editar', employee_payment_edit_path(f['id']) %></TD><TD>
<%= link_to 'Borrar', { :action => :destroy, :id => f.id }, :method => :delete, :data => { :confirm => 'Desea borrar el registro?' } %>
		</TD>
	</tr>
    <% end %>
    </tbody>

</table>
